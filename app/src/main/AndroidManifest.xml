<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.testretro">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/custom">
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity android:name=".MapsActivity"></activity>
        <activity android:name=".SplashScreenActivity"></activity>
        <activity android:name=".InfoActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".alzymerActivity" />
        <activity android:name=".brain_tumorActivity" />
        <activity android:name=".pneumoniaActivity" />
        <activity android:name=".covidActivity" />
        <activity android:name=".breast_cancerActivity" />
        <activity android:name=".eye_blindnessActivity" />
        <activity android:name=".skin_cancerActivity" />
        <activity android:name=".RegisterActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>